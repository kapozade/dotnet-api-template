{
  "ConnectionStrings": {
    "DatabaseConnection": "Server=localhost;database=Supreme;uid=root;pwd=root;Convert Zero Datetime=True;Connection Timeout=15;"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "DotNetCore.CAP.Internal.SubscribeDispatcher": "None",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  //#if (enable-outbox-pattern)
  "RabbitMqSettings": {
    "Host": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  //#endif
  "ApplicationSettings": {
  },
  "RedisSettings": {
    "Host": "localhost",
    "Port": 6379,
    "DbId": 1,
    "User": "User",
    "Password": "Password"
  },
  //#if (enable-open-telemetry)
  "JaegerSettings": {
    "Url": "http://localhost:4317"
  }
  //#endif
}
